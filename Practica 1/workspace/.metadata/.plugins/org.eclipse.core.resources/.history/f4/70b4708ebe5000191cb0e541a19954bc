package AguileraBalderas;

import tools.Vector2d;

public class Nodo {
	public double coste_g;
	public double estimacion_h;
	public double f;
	public Vector2d posicion;
	public Nodo padre;
	public boolean esMuro;
	
	public Nodo(double d,double e,Vector2d pos,Nodo n,boolean muro) {
		coste_g = d;
		estimacion_h = e;
		posicion = pos;
		padre = n;
		f = coste_g + estimacion_h;
		esMuro = muro;
	}
	
	
	
}
