package AguileraBalderas;

import tools.Vector2d;

public class Nodo {
	private int coste_g;
	private int estimacion_h;
	private Vector2d posicion;
	private Vector2d padre;
	
	public Nodo(coste,heuristica,pos,nodo_padre) {
		coste_g = coste;
		estimacion_h = heuristica;
		posicion = pos;
		padre = nodo_padre;
	}
}
